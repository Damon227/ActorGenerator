<?xml version="1.0" encoding="utf-8" ?>
<TemplateSettings>
  <!--
    Type = Folder 生成文件夹
    Type = File 生成文件
    Output 指生成的文件路径
    Source 指文件模板路径
  -->
  <TemplateSetting Name = "StatefulActor">  
    <!--<Template Type = "Folder" Output = "$NameSpaces$\Sql" />-->

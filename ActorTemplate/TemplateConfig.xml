    <Template Type = "File" Output = "$NameSpaces$\Program.cs" Source = "Templates\StatefulActor.Program.txt" />
    <Template Type = "File" Output = "$NameSpaces$\$ActorName$.cs" Source = "Templates\StatefulActor.DemoActor.txt" />
